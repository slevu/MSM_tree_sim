@article{hanson_mean_2016,
	title = {Mean {Recency} {Period} for {Estimation} of {HIV}-1 {Incidence} with the {BED}-{Capture} {EIA} and {Bio}-{Rad} {Avidity} in {Persons} {Diagnosed} in the {United} {States} with {Subtype} {B}},
