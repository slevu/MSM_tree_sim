	keywords = {Cluster Analysis, Contact Tracing, Epidemics, Great Britain, Hiv-1, HIV Infections, Humans, Male, Molecular Epidemiology, Molecular Sequence Data, Molecular Typing, Phylogeny, Sequence Analysis, DNA},
	pages = {1463--1469},
