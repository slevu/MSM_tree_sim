	pmid = {24479899},
	pmcid = {PMC4312901},
	pages = {449--456},
	file = {Seaman_2014_Methods_for.pdf:/Users/slevu/Dropbox/zotero_pdf/Seaman_2014_Methods_for.pdf:application/pdf}
}

