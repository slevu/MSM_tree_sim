@misc{le_vu_characterising_2016,
	address = {Woods Hole, Massachusetts},
	title = {Characterising {HIV} transmission risk factors in men who have sex with men using genetic source attribution and phylogenetic clustering},
	abstract = {Background: Phylogenetic clustering of random samples of HIV sequences can reveal epidemiological transmission patterns, but such analysis has limited theoretical support. Alternative source attribution methods provide unbiased estimates of the probability of each potential transmission pair. 

Methods: We analysed the first pol sequence obtained from 12164 treatment-naive patients diagnosed in London with HIV-1 subtype B and categorized as men who have sex with men. 
Phylogenies were analysed using both conventional distance and tree-based clustering methods using different threshold and source attribution methods which provide an estimate of the probability of each potential transmission pair. Linear and logistic regression models were used to describe trends in clustering and cluster size as a function of patient attributes including CD4, year of diagnosis and age. These analyses were repeated using the estimated number of transmissions attributable to each patient. 

Results: Both clustering and source attribution methods show very small but statistically significant effects of lower age and of higher CD4 at diagnosis on cluster size and attributable transmissions. Year of diagnosis is significantly associated both with cluster size and attributable transmission but in opposite direction: more recent diagnoses tend to belong to larger cluster but tend to have a smaller number of attributable transmissions.
Both approaches show strong assortative mixing between similar age groups and sampling periods but mixing patterns are different regarding CD4 stages at diagnosis.
The distribution of cluster sizes deviates greatly from the estimated number of attributable transmissions and the two quantities are weakly correlated.

Conclusions: While phylogenetic clustering is a fast and convenient tool for studying epidemiological transmission patterns, it has little theoretical support and may provide misleading results. Comparison to source attribution methods reveals some contrasting results that may indicate different transmission patterns.},
	author = {Le Vu, Stephane and Ratmann, Oliver and Delpech, Valerie and Brown, Alison E and Gill, O Noel and Fraser, Christophe and Volz, Erik M.},
	month = apr,
	year = {2016}
}