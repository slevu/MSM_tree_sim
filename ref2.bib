@article{poon_impact_2015,
	title = {The {Impact} of {Clinical}, {Demographic} and {Risk} {Factors} on {Rates} of {HIV} {Transmission}: {A} {Population}-based {Phylogenetic} {Analysis} in {British} {Columbia}, {Canada}},
