	number = {11},
	urldate = {2016-04-04},
	journal = {Journal of Virology},
	author = {Robbins, Kenneth E. and Lemey, Philippe and Pybus, Oliver G. and Jaffe, Harold W. and Youngpairoj, Ae S. and Brown, Teresa M. and Salemi, Marco and Vandamme, Anne-Mieke and Kalish, Marcia L.},
